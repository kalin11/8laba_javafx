<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="lab.client.Controllers.ConnectToServer">
   <children>
      <Label fx:id="label_connect" alignment="CENTER" layoutX="175.0" layoutY="115.0" prefHeight="21.0" prefWidth="249.0" text="%key.connect.label" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="server_host" layoutX="221.0" layoutY="166.0" prefHeight="25.0" prefWidth="158.0" promptText="%key.enter.host" />
      <Label fx:id="label_host" alignment="CENTER" layoutX="239.0" layoutY="145.0" prefHeight="17.0" prefWidth="121.0" text="%key.host.label" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <TextField fx:id="server_port" layoutX="221.0" layoutY="232.0" prefHeight="25.0" prefWidth="158.0" promptText="%key.enter.port" />
      <Label fx:id="label_port" alignment="CENTER" layoutX="239.0" layoutY="211.0" prefHeight="17.0" prefWidth="121.0" text="%key.port.label" textAlignment="CENTER">
         <font>
            <Font size="15.0" />
         </font>
      </Label>
      <Button fx:id="connect_button" layoutX="221.0" layoutY="307.0" mnemonicParsing="false" onAction="#switchToLoginScene" prefHeight="25.0" prefWidth="158.0" text="%key.connect" />
      <Button fx:id="button_ru" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="39.0" text="RU" />
      <Button fx:id="button_eng" layoutX="59.0" layoutY="14.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="46.0" text="ENG" />
      <Button fx:id="button_nor" layoutX="59.0" layoutY="47.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="46.0" text="NOR" />
      <Button fx:id="button_lva" layoutX="14.0" layoutY="47.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="39.0" text="LVA" />
   </children>
</AnchorPane>
